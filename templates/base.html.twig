<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('js/script.js') }}" defer></script>
        {% endblock %}
    </head>
    <body class="default-page">

        <main class="default-wrapper">

            <section class="default-sidebar start">
            
                <div class="neumorphic">
                
                    <button class="btn-neumorphic" id="btn-neumorphic">
                        <span>Start</span>
                    </button>
                    <span class="neumorphic-effet1"></span>
                    <span class="neumorphic-effet2"></span>

                </div>
                <div class="sidebar-wrapper">
                
                    <div class="sidebar-titre"></div>
                    <div class="sidebar-content">
                    
                        <div class="sidebar-item">
                            <h3></h3>
                            <p></p>
                            <button class="btn-sidebar" id="btn-sidebar"></button>
                        </div>

                    </div>

                </div>

            </section>

            <section class="default-main">
            
            </section>

        </main>

    </body>
</html>
